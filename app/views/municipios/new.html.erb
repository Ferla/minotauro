<h1>New municipio</h1>

<% form_for(@municipio) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :codigo %><br />
    <%= f.text_field :codigo %>
  </p>
  <p>
    <%= f.label :departamento %><br />
    <%= f.text_field :departamento %>
  </p>
  <p>
    <%= f.label :municipio %><br />
    <%= f.text_field :municipio %>
  </p>
  <p>
    <%= f.label :descripcion %><br />
    <%= f.text_field :descripcion %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', municipios_path %>