<h1>Listing tiposestados</h1>

<table>
  <tr>
    <th>Clasificacion</th>
    <th>Descripcion</th>
    <th>Estado</th>
  </tr>

<% @tiposestados.each do |tiposestado| %>
  <tr>
    <td><%=h tiposestado.clasificacion %></td>
    <td><%=h tiposestado.descripcion %></td>
    <td><%=h tiposestado.estado %></td>
    <td><%= link_to 'Show', tiposestado %></td>
    <td><%= link_to 'Edit', edit_tiposestado_path(tiposestado) %></td>
    <td><%= link_to 'Destroy', tiposestado, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New tiposestado', new_tiposestado_path %>