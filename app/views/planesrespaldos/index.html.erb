    <td><%=h planesrespaldo.amortiza %></td>
    <td><%=h planesrespaldo.amortiza_ori %></td>
    <td><%=h planesrespaldo.corriente %></td>
    <td><%=h planesrespaldo.corriente_ori %></td>
    <td><%=h planesrespaldo.final %></td>
    <td><%=h planesrespaldo.final_ori %></td>
    <td><%=h planesrespaldo.mora %></td>
    <td><%=h planesrespaldo.diasmora %></td>
    <td><%=h planesrespaldo.moraacum %></td>
    <td><%=h planesrespaldo.morapendiente %></td>
    <td><%=h planesrespaldo.fecha_pago %></td>
    <td><%=h planesrespaldo.valor_pagar %></td>
    <td><%= link_to 'Show', planesrespaldo %></td>
    <td><%= link_to 'Edit', edit_planesrespaldo_path(planesrespaldo) %></td>
    <td><%= link_to 'Destroy', planesrespaldo, :confirm => 'Are you sure?', :method => :delete %></td>
