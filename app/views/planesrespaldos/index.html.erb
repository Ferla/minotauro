    <th>Cuota</th>
    <th>Cuota ori</th>
    <th>Fecha vence</th>
    <th>Inicial</th>
    <th>Inicial ori</th>
    <th>Amortiza</th>
    <th>Amortiza ori</th>
    <th>Corriente</th>
