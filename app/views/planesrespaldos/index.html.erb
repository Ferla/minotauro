    <td><%=h planesrespaldo.planesrecaudo_id %></td>
    <td><%=h planesrespaldo.personasobligacion_id %></td>
    <td><%=h planesrespaldo.persona_id %></td>
    <td><%=h planesrespaldo.nro_cuota %></td>
    <td><%=h planesrespaldo.cuota %></td>
    <td><%=h planesrespaldo.cuota_ori %></td>
    <td><%=h planesrespaldo.fecha_vence %></td>
