<h1>Listing actas</h1>

<table>
  <tr>
    <th>Numero</th>
    <th>Fecha</th>
    <th>Temas</th>
  </tr>

<% @actas.each do |acta| %>
  <tr>
    <td><%=h acta.numero %></td>
    <td><%=h acta.fecha %></td>
    <td><%=h acta.temas %></td>
    <td><%= link_to 'Show', acta %></td>
    <td><%= link_to 'Edit', edit_acta_path(acta) %></td>
    <td><%= link_to 'Destroy', acta, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New acta', new_acta_path %>